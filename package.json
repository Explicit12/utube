    "autoprefixer": "^10.4.13",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^5.1.14",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "husky": ">=6",
    "jsdom": "^21.1.0",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "tailwindcss": "^3.2.1",
    "typescript": "~4.7.4",
    "vite": "^3.1.8",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.0.8"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
