  "dependencies": {
    "@iconify-prerendered/vue-mdi": "^0.13.1665726087",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@vueuse/core": "^9.4.0",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "pinia": "^2.0.0-rc.15",
    "pinia-shared-state": "^0.2.10",
    "vue": "^3.2.41",
    "vue-i18n": "9",
    "vue-plyr": "^7.0.0",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@intlify/eslint-plugin-vue-i18n": "^2.0.0",
    "@intlify/unplugin-vue-i18n": "^0.7.0",
    "@rushstack/eslint-patch": "^1.1.4",
    "@testing-library/vue": "^6.6.1",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^16.11.68",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
